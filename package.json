{
  "name": "enhancedmath",
  "version": "2.11.1",
  "description": "This package contains some enhanced mathematical operations",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "npm run eslint && npm run typecheck",
    "test": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write ./src",
    "eslint": "eslint --fix --ext .ts src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "js",
    "math",
    "probability",
    "algebra",
    "matrix",
    "bisection",
    "discriminator",
    "factorial",
    "fibonacci",
    "combination",
    "permutation",
    "determinant",
    "transpose"
  ],
  "author": "Mout Pessemier",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MoutPessemier/enhancedMathJS/issues"
  },
  "homepage": "https://github.com/MoutPessemier/enhancedMathJS#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoutPessemier/enhancedMathJS.git"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/eslint": "^8.44.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "eslint": "^8.53.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jsdoc": "^4.0.2",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "bignumber.js": "^9.1.2"
  }
}
